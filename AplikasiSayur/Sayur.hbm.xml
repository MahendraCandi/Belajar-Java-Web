<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.ord/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="Sayur" table="sayur">
		<meta attribute="class-description">
			Kelas ini adalah detail dari model sayur.
		</meta>
		<id name="kodeSayur" type="String" column="kdSayur">
			<generator class="native"/>
		</id>
		<property name="namaSayur" column="nmSayur" type="String"/>
		<property name="hargaSayur" column="hrgSayur" type="String"/>
		<property name="stok" column="stok" type="int"/>
	</class>
</hibernate-mapping>